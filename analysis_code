
import pandas as  pd 
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns 


# reading the file
df=pd.read_csv('zomato_data.csv')

# perform data cleaning over column rate 
def handleRate(value):
    value=str(value).split('/')
    value=value[0];
    return float(value)

df['rate']=df['rate'].apply(handleRate)
print(df.head())

